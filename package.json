    "nodemon": "^2.0.16",
    "path": "^0.12.7",
    "prettier": "1.17.0",
    "pubsub": "^3.2.1",
    "sinon": "^7.3.2",
    "ts-jest": "^28.0.3",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "^3.5.2"
