    "mocha": "^10.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "4.16.1",
    "@types/graphql": "^14.2.1",
    "@types/jest": "^24.0.15",
    "@types/node": "11.13.4",
    "@types/supertest": "2.0.7",
    "acorn": "^6.4.1",
    "ajv": "^8.11.0",
